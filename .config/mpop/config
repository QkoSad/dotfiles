defaults
tls on
tls_starttls off

account abv
user malak_rapon@abv.bg
passwordeval "gpg --batch --yes --pinentry-mode loopback --passphrase-file ~/Documents/scripts/pass-gpg.txt -dq ~/.config/mutt/accounts/malakrapon.abv/isync-msmtp.gpg"
host pop3.abv.bg
port 995
delivery maildir "~/.local/share/email/accounts/malakrapon.abv/INBOX"
timeout 10

